<Window x:Class="VibeProxy.Windows.QwenEmailDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Qwen Email"
        WindowStartupLocation="CenterOwner"
        SizeToContent="WidthAndHeight"
        ResizeMode="NoResize">
    <StackPanel Margin="24" Width="320">
        <TextBlock Text="Enter your Qwen account email" FontWeight="SemiBold" />
        <TextBox x:Name="EmailBox" Margin="0,8,0,0" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,12,0,0">
            <Button Content="Cancel" Click="OnCancel" Margin="0,0,8,0" />
            <Button Content="Continue" IsDefault="True" Click="OnContinue" />
        </StackPanel>
    </StackPanel>
</Window>
